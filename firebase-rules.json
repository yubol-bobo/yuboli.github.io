{
  "rules": {
    "impressions": {
      "words": {
        ".read": true,
        "$word": {
          ".write": "!data.exists() || newData.val() > data.val()",
          ".validate": "newData.isNumber() && newData.val() >= 0 && newData.val() <= data.val() + 10"
        }
      }
    }
  }
}
